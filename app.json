{
  "expo": {
    "name": "Solomon Islands Sign Language",
    "slug": "solomon-islands-sign-lanugage",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "scheme": "sislep",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "org.sislep"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "org.sislep"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-asset",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/splash-icon.png"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "56afb791-455c-4890-9ee2-5f42d54f0186"
      }
    },
    "owner": "solomon-islands-deaf-association"
  }
}
